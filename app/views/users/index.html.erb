<% provide(:title, t('.text')) %>
<h1 class="h1_us_index"><%= t('.text') %></h1>

<div class="users_main col-md-10 row">
  <% if @users.present? %>
    <%= render @users, cache: true %>
  <% else %>
    <h2 class="col-10 text-center"><b><%= t('.not_found') %></b></h2>
  <% end %>
</div>
<%= paginate @users %>
