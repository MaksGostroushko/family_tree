<% provide(:title, "Sign up") %>
<h1 class="h_session"><%= t('.signup') %></h1>

    <%= form_for(@user, url: signup_path) do |f| %>
    <%= render 'shared/errors' %>

    <div class="form-group">
      <%= f.label :name %>
      <br>
      <%= f.text_field :name, class: 'form-control', placeholder: t('.label_text') %>
    </div>

    <div class="form-group">
      <%= f.label :second_name %>
      <br>
      <%= f.text_field :second_name, class: 'form-control', placeholder: t('.label_text_two') %>
    </div>

      <p></p>

    <div class="form-group">
      <%= f.label :email %>
      <br>
      <%= f.email_field :email, class: 'form-control', placeholder: t('.e_email') %>
    </div>

      <p></p>

    <div class="form-group">
      <%= f.label :password %>
      <br>
      <%= f.password_field :password, class: 'form-control', placeholder: t('.e_pass') %>
    </div>

      <p></p>
    <div class="form-group">
      <%= f.label :password_confirmation %>
      <br>
      <%= f.password_field :password_confirmation, class: 'form-control' %>
    </div>

    <div class="form-group">
      <%= f.label :image %>
      <%= f.file_field :image %>
    </div>

      <p></p>

    <div class="text-center">
      <%= f.submit t('.new_ac'), class: "btn btn-primary" %>
    </div>

    <p></p>

    <!-- <p class='text-center'>Have account? <%#= link_to "Log in right now!", login_path %></p> -->
    <% end %>
