<% if logged_in? %>
  <div class="row">
    <div class="col-md-2">
      <%= render 'categories' %>
    </div>
    <div class="users_main col-md-10 row">
        <% if @relatives.present? %>
          <%= render @relatives %>
        <% else %>
          <h2 class="col-10 text-center"><b>No people</b></h2>
        <% end %>
    </div>
  </div>
<% else %>
  <%= render 'sessions/login' %>
<% end %>
